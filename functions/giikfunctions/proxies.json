{
    "$schema": "http://json.schemastore.org/proxies",
    "proxies": {
      "ReadDownloadCountByBlobProxy": {
        "matchCondition": {
          "route": "/podcast/{blobName}/downloadcount",
          "methods": [
            "GET"
          ]
        },
        "backendUri": "http://api.giik.fm/api/ReadDownloadCountByBlob?blob={blobName}"
      }
    }
}